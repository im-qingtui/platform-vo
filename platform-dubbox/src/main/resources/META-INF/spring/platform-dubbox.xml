<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:apollo="http://www.ctrip.com/schema/apollo" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                         http://www.ctrip.com/schema/apollo http://www.ctrip.com/schema/apollo.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

  <apollo:config namespaces="application,QT.common"/>

  <dubbo:protocol port="${dubbo.protocol.port:-1}"/>

  <dubbo:protocol name="dubbo" port="-1"/>

  <dubbo:consumer check="false"/>

  <dubbo:annotation package="im.qingtui"/>

  <dubbo:registry address="${zookeeper.url}"/>

</beans>